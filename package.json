{
  "name": "sveltekit-themes",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "publish": "pnpm package && changeset version && changeset publish",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.4.1",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sveltekit",
    "typescript",
    "svelte",
    "sveltekit-themes"
  ],
  "homepage": "https://github.com/thejessewinton/sveltekit-themes",
  "bugs": {
    "url": "https://github.com/thejessewinton/sveltekit-themes/issues"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
